alias furnace d0
alias ledPresGood d1
alias ledTempGood d2

define targetPresMin 50000
define targetPresMax 100000
define targetTempMin 400
define targetTempMax 800

loop:
yield

l r0 furnace Temperature
sgt r1 r0 targetTempMin
slt r2 r0 targetTempMax
and r0 r1 r2
s ledTempGood On r0

l r0 furnace Pressure
sgt r1 r0 targetPresMin
slt r2 r0 targetPresMax
and r0 r1 r2
s ledPresGood On r0

j loop